@model List<ProjectModel>
<div id="sidebar-wrapper">
    <ul class="sidebar-nav">
        <li class="sidebar-brand">
            <a href="#">
                Проекты
            </a>
        </li>
        @foreach (var t in Model)
        {
            <li class="@(t.Selected?"selected":"")">
                <a href="@Url.Action(t.Key, "project")" title="@t.Description" data-url="@t.Link">@t.Name</a>
            </li>
        }
    </ul>
</div>